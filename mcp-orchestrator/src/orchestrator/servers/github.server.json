{
  "name": "github",
  "type": "mcp-server",
  "scopes": [
    "repo:read",
    "repo:write",
    "pull_request",
    "projectV2",
    "issues"
  ],
  "config": {
    "base_url": "https://api.github.com",
    "owner": "YOUR_ORG_OR_USER",
    "repo": "YOUR_REPO",
    "default_base_branch": "dev"
  },
  "auth": {
    "mode": "token",
    "env": "GH_TOKEN"
  }
}