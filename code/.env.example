# Configuraci√≥n de variables de entorno para Yega GitHub API

# GitHub Configuration
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_OWNER=yega
GITHUB_API_URL=https://api.github.com/
GITHUB_TIMEOUT=30

# Redis Configuration
REDIS_HOST=127.0.0.1
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# Logging
LOG_LEVEL=info
LOG_PATH=/var/log/yega-github-api

# Cache Configuration
CACHE_DEFAULT_TTL=300
CACHE_REPO_TTL=600
CACHE_STATS_TTL=3600

# API Configuration
API_RATE_LIMIT=5000
API_RATE_LIMIT_WINDOW=3600

# Environment
APP_ENV=production
APP_DEBUG=false